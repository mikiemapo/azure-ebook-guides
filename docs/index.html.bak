        }
        50% {
          box-shadow: 0 4px 20px rgba(233, 30, 99, 0.6);
        }
      }

      .media-library-intro {
        background: rgba(255, 255, 255, 0.15);
        padding: 15px;
        border-radius: 8px;
        margin-bottom: 20px;
        line-height: 1.6;
      }
    </style>
