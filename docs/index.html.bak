      function loadMediaFiles() {
        try {
          console.log(
            "Loading media files from embedded data:",
            EMBEDDED_MEDIA_DATA.length,
            "files"
          );

          const mediaByDomain = {
            Networking: [],
            Compute: [],
            Storage: [],
            Identity: [],
            Governance: [],
            Monitor: [],
            General: [],
          };

          EMBEDDED_MEDIA_DATA.forEach((row) => {
            const domain = row.Domain;
            const newName = row.New_Name;
            const type = row.Type;

            if (!mediaByDomain[domain]) return;

            const cleanedName = newName
              .replace(/^AZ-104-[^-]+-/, "")
              .replace(/_/g, " ")
              .replace(/\.[^.]+$/, "");

            const icon =
              type === "MP4" || type === "MOV" ? "fa-video" : "fa-headphones";
            const path = `../Conversations /AZ-104 CONVERSATIONS/${newName}`;

            mediaByDomain[domain].push({
              name: cleanedName,
              path: path,
              type: type,
              icon: icon,
            });
          });

          // Render files by domain
          Object.keys(mediaByDomain).forEach((domain) => {
            const container = document.getElementById(
              `media-${domain.toLowerCase()}`
            );
            if (!container) return;

            const files = mediaByDomain[domain].sort((a, b) =>
              a.name.localeCompare(b.name)
            );

            files.forEach((file) => {
              const mediaItem = document.createElement("div");
              mediaItem.className = "media-item";
              mediaItem.innerHTML = `
                <i class="fas ${file.icon}"></i>
                <a href="${file.path}" target="_blank">${file.name}</a>
                <span class="media-type-badge">${file.type}</span>
              `;
              container.appendChild(mediaItem);
            });
          });

          console.log("Media library loaded successfully!");
        } catch (error) {
          console.error("Error loading media catalog:", error);
          document.getElementById("media-content").innerHTML = `
            <div style="padding: 20px; text-align: center; color: rgba(255,255,255,0.9);">
              <i class="fas fa-exclamation-triangle"></i>
              <p>Unable to load media catalog.</p>
              <p style="font-size: 0.9em; margin-top: 10px;">Error: ${error.message}</p>
            </div>
          `;
        }
      }
    </script>
